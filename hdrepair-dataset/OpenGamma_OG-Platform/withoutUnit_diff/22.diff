index 0000000..922684b
--- /dev/null
+++ b/projects/OG-Integration/config/htssnapshot/hts-snapshot-infrastructure-spring.xml
@@ -0,0 +1,64 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<beans xmlns="http://www.springframework.org/schema/beans"
+       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
+       xsi:schemaLocation="
+http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
+
+  <!-- ================================================================== -->
+  <bean id="standardFudgeContext" class="com.opengamma.util.fudgemsg.OpenGammaFudgeContext" factory-method="getInstance" />
+    
+  <!-- ================================================================== -->
+  
+  <bean id="htsDataSource" class="com.jolbox.bonecp.BoneCPDataSource" destroy-method="close">
+    <property name="driverClass" value="${db.hts.driver}" />
+    <property name="jdbcUrl" value="${db.hts.url}" />
+    <property name="username" value="${db.hts.username}" />
+    <property name="password" value="${db.hts.password}" />
+    <property name="poolName" value="TimeSeries"/>
+    <property name="partitionCount" value="1" />
+    <property name="acquireIncrement" value="1" />
+    <property name="minConnectionsPerPartition" value="1" />
+    <property name="maxConnectionsPerPartition" value="10" />
+  </bean>
+
+  <bean id="htsDbConnector" class="com.opengamma.util.db.DbConnectorFactoryBean">
+    <property name="name" value="TimeSeries"/>
+    <property name="dataSource" ref="htsDataSource"/>
+    <property name="dialectName" value="${db.hts.dialect}"/>
+    <property name="transactionIsolationLevelName" value="ISOLATION_READ_COMMITTED" />
+    <property name="transactionPropagationBehaviorName" value="PROPAGATION_REQUIRED" />
+  </bean>
+  
+  <bean id="jedisConfig" class="redis.clients.jedis.JedisPoolConfig"> 
+    <property name="testWhileIdle" value="true"/> 
+    <property name="minEvictableIdleTimeMillis" value="60000"/>
+    <property name="timeBetweenEvictionRunsMillis" value="30000"/>
+    <property name="numTestsPerEvictionRun" value="-1"/>
+  </bean>
+   
+  <bean id="htsRedisConnector" class="com.opengamma.util.redis.RedisConnectorFactoryBean" >
+    <property name="name" value="Standard"/>
+    <property name="config" ref="jedisConfig"/>
+    <property name="host" value="${redis.host}"/>
+    <property name="port" value="${redis.port}"/>
+  </bean>
+     
+  <bean id="htsSchemeBlackList" class="com.opengamma.integration.timeseries.snapshot.SchemeBlackListFactoryBean">
+    <property name="name" value="HtsSchemeBlackList"/>
+    <property name="schemeBlackList">
+      <list >
+        <value>test</value>
+      </list>
+    </property>
+  </bean>
+  
+  <bean id="htsDataFieldBlackList" class="com.opengamma.integration.timeseries.snapshot.DataFieldBlackListFactoryBean">
+    <property name="name" value="HtsDataFieldBlackList"/>
+    <property name="dataFieldBlackList">
+      <list >
+        <value>test</value>
+      </list>
+    </property>
+  </bean>
+  
+</beans>
\ No newline at end of file
diff --git a/projects/OG-Integration/config/htssnapshot/hts-snapshot-logback.xml b/projects/OG-Integration/config/htssnapshot/hts-snapshot-logback.xml
new file mode 100644
index 0000000..1b6a169
--- /dev/null
+++ b/projects/OG-Integration/config/htssnapshot/hts-snapshot-logback.xml
@@ -0,0 +1,25 @@
+<configuration>
+
+  <contextName>HTSSnapshotter</contextName>
+  <jmxConfigurator />
+
+
+  <appender name="FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">
+    <file>htssnapshotter.log</file>
+    <rollingPolicy class="ch.qos.logback.core.rolling.FixedWindowRollingPolicy">
+      <fileNamePattern>htssnapshotter.%i.log.gz</fileNamePattern>
+      <minIndex>1</minIndex>
+      <maxIndex>9</maxIndex>
+    </rollingPolicy>
+    <triggeringPolicy class="ch.qos.logback.core.rolling.SizeBasedTriggeringPolicy">
+      <maxFileSize>50MB</maxFileSize>
+    </triggeringPolicy>
+    <layout class="ch.qos.logback.classic.PatternLayout">
+      <Pattern>%date [%thread] %-5level %logger{36} - %msg%n</Pattern>
+    </layout>
+  </appender>
+
+  <root level="warn">
+    <appender-ref ref="FILE" />
+  </root>
+</configuration>
diff --git a/projects/OG-Integration/config/htssnapshot/hts-snapshot.ini b/projects/OG-Integration/config/htssnapshot/hts-snapshot.ini
new file mode 100644
index 0000000..0b08cb1
--- /dev/null
+++ b/projects/OG-Integration/config/htssnapshot/hts-snapshot.ini
@@ -0,0 +1,41 @@
+# Configuration to setup a historical timeseries snaphotter
+
+[global]
+time.zone = Europe/London
+
+[infrastructure]
+factory = com.opengamma.integration.timeseries.snapshot.SpringHtsSnapshotInfrastructureComponentFactory
+springFile = ${component.infrastructure.springFile}
+propertiesFile = ${component.infrastructure.propertiesFile}
+
+#============================================================================
+[dbHtsMaster]
+factory = com.opengamma.component.factory.master.DbHistoricalTimeSeriesMasterComponentFactory
+classifier = central
+dbConnector = ::hts
+
+#============================================================================
+[htsSnapshotterJobDetail]
+factory = com.opengamma.integration.timeseries.snapshot.JobDetailComponentFactory
+classifier = hts
+name = HtsJobDetail
+htsMaster = ::central
+redisConnector = ::hts
+
+#============================================================================
+[londonCloseSnapshot]
+factory = com.opengamma.integration.timeseries.snapshot.CronTriggerComponentFactory
+classifier = standard
+jobDetail = ::hts
+name = LondonClose
+cronExpression = ${londonCloseSnapshot.cronExpression}
+dataSource = ${londonCloseSnapshot.datasource}
+normalizationRuleSetId = ${londonCloseSnapshot.normalizationRuleSetId}
+observationTime = ${londonCloseSnapshot.observationTime}
+schemeBlackList = SchemeBlackList::hts
+dataFieldBlackList = ::hts
+
+#============================================================================
+[htsSnapshotterScheduler]
+factory = com.opengamma.integration.timeseries.snapshot.SchedulerComponentFactory
+classifier = standard
\ No newline at end of file
diff --git a/projects/OG-Integration/config/htssnapshot/hts-snapshot.properties b/projects/OG-Integration/config/htssnapshot/hts-snapshot.properties
new file mode 100644
index 0000000..8a5cb51
--- /dev/null
+++ b/projects/OG-Integration/config/htssnapshot/hts-snapshot.properties
@@ -0,0 +1,27 @@
+# Next configuration file in the chain is the INI file
+MANAGER.NEXT.FILE = classpath:htssnapshot/hts-snapshot.ini
+
+component.infrastructure.springFile = classpath:htssnapshot/hts-snapshot-infrastructure-spring.xml
+component.infrastructure.propertiesFile = MANAGER.PROPERTIES
+
+component.jetty.propertiesFile = MANAGER.PROPERTIES
+
+#db.hts.dialect = com.opengamma.util.db.PostgresDbDialect
+#db.hts.driver = org.postgresql.Driver
+#db.hts.url = jdbc:postgresql://postgresql-lx-1.hq.opengamma.com/og_tss
+#db.hts.username = tssowner
+#db.hts.password = ajT0xnJj
+
+db.hts.dialect = com.opengamma.util.db.HSQLDbDialect
+db.hts.driver = org.hsqldb.jdbcDriver
+db.hts.url = jdbc:hsqldb:file:install/db/hsqldb/example-db
+db.hts.username = OpenGamma
+db.hts.password = OpenGamma
+
+redis.host=redis-lx-1.hq.opengamma.com
+redis.port=6379
+
+londonCloseSnapshot.cronExpression=0 12 16 ? * MON-FRI *
+londonCloseSnapshot.datasource=OPENGAMMA
+londonCloseSnapshot.normalizationRuleSetId=None
+londonCloseSnapshot.observationTime=LON_CLOSE
